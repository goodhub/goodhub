{
  "name": "@strawberrylemonade/goodhub-lib",
  "private": false,
  "version": "1.35.0",
  "description": "A shared library for GoodHub types, shared functions and services. Backend & Frontend.",
  "author": "strawberrylemonade",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/strawberrylemonade/goodhub-lib"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": false
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "npx microbundle-crl --no-compress --format modern,cjs",
    "start": "npx microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "npm run build",
    "docs": "npx typedoc --out docs src/index.ts",
    "predeploy": "cd example && npm install && npm run build"
  },
  "peerDependencies": {},
  "devDependencies": {
    "microbundle-crl": "^0.13.10",
    "prettier": "^2.0.4",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@sentry/core": "^6.2.2"
  }
}
